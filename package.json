{
  "name": "@base/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx run-many --target=serve --projects=server,frontend --parallel --maxParallel=2",
    "typeorm:migration": "cross-env TS_NODE_PROJECT=libs/server/database/tsconfig.json node --require ts-node/register ./node_modules/typeorm/cli.js -d libs/server/database/src/lib/migration.ds.ts",
    "typeorm:seed": "cross-env TS_NODE_PROJECT=libs/server/database/tsconfig.json node --require ts-node/register ./node_modules/typeorm/cli.js -d libs/server/database/src/lib/seed.ds.ts",
    "db:migration:create": "cross-env TS_NODE_PROJECT=libs/server/database/tsconfig.json node --require ts-node/register ./node_modules/typeorm/cli.js migration:create",
    "db:migrate": "yarn typeorm:migration migration:run",
    "db:migrate:rollback": "yarn typeorm:migration migration:revert",
    "db:seed:create": "yarn db:migration:create",
    "db:seed": "yarn typeorm:seed migration:run",
    "db:seed:rollback": "yarn typeorm:seed migration:revert",
    "db:refresh": "yarn db:migrate && yarn db:seed"
  },
  "private": true,
